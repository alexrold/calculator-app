{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["components/*"],
      "@component/*": ["components/*"],
      "@hooks/*": ["components/hooks/*"],
      "@constants/*": ["constants/*"],
      "@styles/*": ["styles/*"],
      "@assets/*": ["assets/*"],
      "@app/*": ["app/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
